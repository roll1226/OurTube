{
  "name": "ourtube",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:emulators": "NEXT_PUBLIC_USE_FIREBASE_EMULATOR=true next dev",
    "dev:firebase": "NEXT_PUBLIC_USE_FIREBASE_EMULATOR=false next dev",
    "build": "next build",
    "start": "next start",
    "build-functions": "cd functions && npm run build",
    "emulators": "yarn build-functions && firebase emulators:start",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:rule-firestore": "firebase deploy --only firestore:rules",
    "deploy:rule-database": "firebase deploy --only database",
    "deploy:rule-storage": "firebase deploy --only storage",
    "prepare": "husky install",
    "lint": "eslint src/**/*.{ts,tsx}",
    "check-types": "tsc --noEmit"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@reduxjs/toolkit": "^1.6.0",
    "@types/node": "^16.7.10",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "firebase": "^8.7.0",
    "js-base64": "^3.6.1",
    "next": "10.2.3",
    "react": "17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "17.0.2",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.4",
    "react-toastify": "^8.0.2",
    "react-youtube": "^7.13.1",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.3.0",
    "styled-reset": "^4.3.4",
    "use-media": "^1.4.0",
    "yt-player": "^3.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.11",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/react": "^17.0.9",
    "@types/react-redux": "^7.1.17",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
